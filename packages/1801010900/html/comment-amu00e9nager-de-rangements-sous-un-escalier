<script type="text/javascript">
dojo.ready(function(){
	
	$('form[name=subscribeNewsletterForm]').each(function( index ) {
		$(this).validate({
			onfocusout: false,
			onkeyup: false,
			onclick: false,
			rules : {					
				email : {
					required : true,
					email : true,
					maxlength : 100
				},
				firstName : {
					maxlength : 80
				},
				lastName : {
					maxlength : 80
				}
			},
			messages : {
				email : {
					required : "Veuillez renseigner une adresse email",
					email : "Votre adresse email n'est pas valide",
					maxlength : "Votre adresse email est trop longue"
				},
				firstName:{
					maxlength : "Votre prénom est trop long"
				},
				lastName:{
					maxlength : "Votre nom est trop long"
				}
			
			},
			errorPlacement : function(error, element) {
				$("#subscribeNewsletterErrorMessageArea p").addClass("hidden");
				error.insertAfter(".newsletter-errors");
			},
			submitHandler : function(form) {
				dataLayer.push({"event":"newsletterSubscription","newsletterPopinStep":"Submit"});
				
				subscribeNewsletter(form);
			}
		});
	});
	
});

function pushSocialNetworkFollow(socialNetwork_name){
	dataLayer.push({"event":"socialFollow", "nom_reseau_social":socialNetwork_name});
}

</script>


