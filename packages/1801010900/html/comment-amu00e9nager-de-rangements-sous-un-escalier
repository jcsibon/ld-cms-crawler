<script type="text/javascript">
	dojo.addOnLoad(function() { 
		//Make sure page is loaded at this point
		//Set requestedSubmitted to false
		resetRequest();

		// All div's whose id attribute contains dojoWidget subString -- dojo.query('div[id*="dojoWidget"]')
		// All div's which contains dojoType attribute -- dojo.query(div[dojoType])
		dojo.query('div[dojoType]').forEach(function(node, index, arr){
			addToWidgetsList(node.id);
		});
		parseAllWidgets();
		if(typeof(stockAvailability) != 'undefined') {
			$(stockAvailability).trigger("onComputeTask_Event");
		}
	}); 	
</script><!-- END Footer.jsp --><!--  Begin TagManager.jsp -->

<script type="text/javascript">

/*! Hack : Reloads on every visit in mobile safari */
if ((/ipad|iphone|ipod/gi).test(navigator.appVersion)) {
	window.onpageshow = function(evt) {
		if (evt.persisted) {
			location.reload();
		}
	};
}

setDeleteCartCookie();
updateCartCookie();
</script>




