					<script>
						function createOrAdd(){
							var errArea= document.getElementById('shoppingListErrorMessageArea');
							errArea.style.display='none';
							var listIdSelect = $('#existingListDP');
							if(listIdSelect == null || $(listIdSelect).children().length == 0) {
								listNameInput=document.getElementById("newListName");
								if(listNameInput == null || listNameInput.value == null || listNameInput.value == '') {
									errArea.innerHTML='<span id="shoppingListErrorMessageText" class="error">' + "Vous n'avez pas entré de nom" + '</span>';
									errArea.style.display='block';
									return false;
								}
							}
							wishList.createOrAddToList("newListName", 'existingListDP', '');
						}
					</script>
				</div>
