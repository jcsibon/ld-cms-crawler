<script>
	function setCoookieForConsentement() {
		dojo.cookie('cookieConsentement', 'true', {path:'/', expires: 365});
		$('#cookieConsentementContainer').fadeOut('slow', function(){
			$('#cookieConsentementContainer').remove();
		});
	}
</script>
